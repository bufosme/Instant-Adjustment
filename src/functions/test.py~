#!/usr/bin/python
# -*- coding: utf-8 -*-


logfile = open("pol_datas", "r").readlines()
points = []
lines = []
words = []
counter = 0
for content in logfile:
    for line in content.split("<br>"):
        lines.append(line)
        for word in lines[counter].split("/"):
            words.append(word)

print lines
